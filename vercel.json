{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/hmr/llms(.+)",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": ".*text/html.*"
        }
      ],
      "missing": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(ai|bot).*"
        }
      ],
      "destination": "/hmr/llms",
      "permanent": false
    }
  ]
}
